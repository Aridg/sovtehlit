   <children>
      <TableView fx:id="customersTable" prefHeight="400.0" prefWidth="505.0">
        <columns>
          <TableColumn fx:id="nameColumn" prefWidth="505.0" text="Наименование" />
        </columns>
      </TableView>
      <Button layoutX="540.0" layoutY="91.0" mnemonicParsing="false" onAction="#onUpdateClick" prefHeight="25.0" prefWidth="25.0" style="-fx-background-image: url('icons/refresh.png');" />
      <Button layoutX="518.0" layoutY="130.0" mnemonicParsing="false" onAction="#onAddClick" text="Добавить" />
      <Button layoutX="517.0" layoutY="175.0" mnemonicParsing="false" onAction="#onEditClick" text="Изменить" />
      <Button layoutX="518.0" layoutY="222.0" mnemonicParsing="false" onAction="#onDelClick" prefHeight="25.0" prefWidth="69.0" text="Удалить" />
   </children>
</AnchorPane>
