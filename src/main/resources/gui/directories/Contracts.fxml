<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="532.0" prefWidth="620.0" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="code.gui.controllers.directories.ContractsController">
    <children>
        <TableView fx:id="contractTable" layoutX="14.0" layoutY="130.0" prefHeight="400.0" prefWidth="500.0">
            <columns>
                <TableColumn fx:id="nameColumn" prefWidth="250.0" text="Наименование" />
            <TableColumn fx:id="dateColumn" prefWidth="250.0" text="Дата" />
            </columns>
        </TableView>
        <Button layoutX="526.0" layoutY="274.0" mnemonicParsing="false" onAction="#onAddClick" text="Добавить" />
        <Button layoutX="525.0" layoutY="319.0" mnemonicParsing="false" onAction="#onEditClick" text="Изменить" />
        <Button layoutX="526.0" layoutY="366.0" mnemonicParsing="false" onAction="#onDelClick" prefHeight="25.0" prefWidth="69.0" text="Удалить" />
      <ComboBox fx:id="customers" layoutX="14.0" layoutY="41.0" onAction="#onCustomerChange" prefHeight="25.0" prefWidth="500.0" />
      <Label layoutX="239.0" layoutY="24.0" text="Заказчик" />
      <Button layoutX="548.0" layoutY="226.0" mnemonicParsing="false" onAction="#onUpdateClick" prefHeight="25.0" prefWidth="25.0" style="-fx-background-image: url('icons/refresh.png');" />
    </children>
</AnchorPane>
