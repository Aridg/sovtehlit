<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="code.gui.controllers.directories.UnitMeasurementController">
   <children>
      <TableView fx:id="unitsTable" prefHeight="407.0" prefWidth="500.0">
         <columns>
            <TableColumn fx:id="idColumn" prefWidth="75.0" text="№" />
            <TableColumn fx:id="nameColumn" prefWidth="424.0" text="Наименование" />
            <TableColumn fx:id="factorColumn" prefWidth="75.0" text="Множитель" />
         </columns>
      </TableView>
      <Button layoutX="540.0" layoutY="83.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="25.0" style="-fx-background-image: url('icons/refresh.png');" />
      <Button layoutX="518.0" layoutY="130.0" mnemonicParsing="false" onAction="#onAddClick" text="Добавить" />
      <Button layoutX="517.0" layoutY="175.0" mnemonicParsing="false" onAction="#onEditClick" text="Изменить" />
      <Button layoutX="518.0" layoutY="222.0" mnemonicParsing="false" onAction="#onDelClick" prefHeight="25.0" prefWidth="69.0" text="Удалить" />
   </children>
</AnchorPane>
